{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API for e-commerce application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:unit": "jest --testMatch='**/__tests__/**/*.test.js'",
    "test:integration": "jest --testMatch='**/__tests__/**/*.integration.test.js'",
    "test:api": "jest --testMatch='**/__tests__/**/*.api.test.js'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "build": "babel src -d dist",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "node",
    "express",
    "mysql",
    "sequelize",
    "redis",
    "jwt",
    "e-commerce"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.6.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.9.2",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "sequelize": "^6.37.1",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.0",
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/preset-env": "^7.24.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.2.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
